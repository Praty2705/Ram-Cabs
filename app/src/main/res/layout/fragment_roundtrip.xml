<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragemnt.RoundtripFragment">

   <androidx.constraintlayout.widget.ConstraintLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent">


      <com.google.android.material.textfield.TextInputLayout
          android:id="@+id/roundtripdate"
          style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:layout_marginTop="40dp"
          android:layout_marginRight="10dp"
          android:fontFamily="@font/abril_fatface"
          android:hint="@string/select_pickup_date"
          android:textColorHint="#8C8989"
          app:boxCornerRadiusBottomEnd="8dp"
          app:boxCornerRadiusBottomStart="8dp"
          app:boxCornerRadiusTopEnd="8dp"
          app:boxCornerRadiusTopStart="8dp"
          app:boxStrokeColor="#8C8989"
          app:boxStrokeWidth="0.5dp"
          app:hintTextColor="#8C8989"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.526"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent"
          tools:ignore="MissingConstraints">


         <com.google.android.material.textfield.TextInputEditText
             android:id="@+id/roundtripdateET"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_marginTop="100dp"
             android:focusable="false"
             android:fontFamily="@font/abril_fatface"
             android:inputType="number"
             android:maxLength="10"
             android:singleLine="true"
             android:textColor="@color/black"
             android:textSize="15sp"
             android:textStyle="bold"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintHorizontal_bias="0.736"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="@+id/textView40" />


      </com.google.android.material.textfield.TextInputLayout>

      <com.google.android.material.textfield.TextInputLayout
          android:id="@+id/roundtripenddate"
          style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:layout_marginTop="16dp"
          android:layout_marginRight="10dp"
          android:fontFamily="@font/abril_fatface"
          android:hint="Return Date"
          android:textColorHint="#8C8989"
          app:boxCornerRadiusBottomEnd="8dp"
          app:boxCornerRadiusBottomStart="8dp"
          app:boxCornerRadiusTopEnd="8dp"
          app:boxCornerRadiusTopStart="8dp"
          app:boxStrokeColor="#8C8989"
          app:boxStrokeWidth="0.5dp"
          app:hintTextColor="#8C8989"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.842"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/roundtriptime"
          tools:ignore="MissingConstraints">


         <com.google.android.material.textfield.TextInputEditText
             android:id="@+id/roundtripenddateET"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_marginTop="100dp"
             android:focusable="false"
             android:fontFamily="@font/abril_fatface"
             android:inputType="number"
             android:maxLength="10"
             android:singleLine="true"
             android:textColor="@color/black"
             android:textSize="15sp"
             android:textStyle="bold"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintHorizontal_bias="0.736"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="@+id/textView40" />


      </com.google.android.material.textfield.TextInputLayout>

      <com.google.android.material.textfield.TextInputLayout
          android:id="@+id/roundtriptime"
          style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:layout_marginTop="16dp"
          android:layout_marginRight="10dp"
          android:fontFamily="@font/abril_fatface"
          android:hint="@string/select_pickup_time"
          android:textColorHint="#8C8989"
          app:boxCornerRadiusBottomEnd="8dp"
          app:boxCornerRadiusBottomStart="8dp"
          app:boxCornerRadiusTopEnd="8dp"
          app:boxCornerRadiusTopStart="8dp"
          app:boxStrokeColor="#8C8989"
          app:boxStrokeWidth="0.5dp"
          app:hintTextColor="#8C8989"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.526"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/roundtripdate"
          tools:ignore="MissingConstraints">


         <com.google.android.material.textfield.TextInputEditText
             android:id="@+id/roundtriptimeET"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_marginTop="100dp"
             android:focusable="false"
             android:fontFamily="@font/abril_fatface"
             android:inputType="number"
             android:maxLength="10"
             android:singleLine="true"
             android:textColor="@color/black"
             android:textSize="15sp"
             android:textStyle="bold"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintHorizontal_bias="0.736"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="@+id/textView40" />


      </com.google.android.material.textfield.TextInputLayout>

      <com.google.android.material.textfield.TextInputLayout
          android:id="@+id/roundtripstartcity"
          style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:layout_marginTop="16dp"
          android:layout_marginRight="10dp"
          android:fontFamily="@font/abril_fatface"
          android:hint="@string/pickup_location"
          android:textColorHint="#8C8989"
          app:boxCornerRadiusBottomEnd="8dp"
          app:boxCornerRadiusBottomStart="8dp"
          app:boxCornerRadiusTopEnd="8dp"
          app:boxCornerRadiusTopStart="8dp"
          app:boxStrokeColor="#8C8989"
          app:boxStrokeWidth="0.5dp"
          app:hintTextColor="#8C8989"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.526"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/roundtripenddate"
          tools:ignore="MissingConstraints">


         <com.google.android.material.textfield.TextInputEditText
             android:id="@+id/roundtripstartcityET"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_marginTop="100dp"
             android:fontFamily="@font/abril_fatface"
             android:singleLine="true"
             android:textColor="@color/black"
             android:textSize="15sp"
             android:textStyle="bold"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintHorizontal_bias="0.736"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="@+id/textView40" />


      </com.google.android.material.textfield.TextInputLayout>
      <androidx.cardview.widget.CardView
          android:id="@+id/cardView6"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:visibility="gone"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.0"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/roundtripstartcity">

         <TextView
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_gravity="left"
             android:layout_marginStart="15dp"
             android:fontFamily="@font/abril_fatface"
             android:text="VIA : "
             android:textColor="@color/black"
             android:textSize="15sp"
             android:textStyle="bold" />

         <TextView
             android:id="@+id/roundtripaddcity1"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginStart="60dp"
             android:fontFamily="@font/abril_fatface"
             android:text="Lucknow"
             android:textColor="@color/black"
             android:textSize="15sp" />

         <ImageView
             android:id="@+id/roundtripcrossIV"
             android:layout_width="50dp"
             android:layout_height="25dp"
             android:layout_gravity="right"
             android:layout_marginRight="15dp"
             android:layout_marginBottom="5dp"
             android:src="@drawable/cross"
             tools:ignore="RtlHardcoded" />

      </androidx.cardview.widget.CardView>


      <com.google.android.material.textfield.TextInputLayout
          android:id="@+id/roundtripendcity"
          style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="10dp"
          android:layout_marginTop="20dp"
          android:layout_marginRight="10dp"
          android:fontFamily="@font/abril_fatface"
          android:hint="@string/destnination_city"
          android:textColorHint="#8C8989"
          app:boxCornerRadiusBottomEnd="8dp"
          app:boxCornerRadiusBottomStart="8dp"
          app:boxCornerRadiusTopEnd="8dp"
          app:boxCornerRadiusTopStart="8dp"
          app:boxStrokeColor="#8C8989"
          app:boxStrokeWidth="0.5dp"
          app:hintTextColor="#8C8989"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.526"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/cardView6"
          tools:ignore="MissingConstraints">


         <com.google.android.material.textfield.TextInputEditText
             android:id="@+id/roundtripendcityET"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_marginTop="100dp"
             android:fontFamily="@font/abril_fatface"
             android:singleLine="true"
             android:textColor="@color/black"
             android:textSize="15sp"
             android:textStyle="bold"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintHorizontal_bias="0.736"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="@+id/textView40" />


      </com.google.android.material.textfield.TextInputLayout>


      <Button
          android:id="@+id/roundtripaddcityBTN"
          android:layout_width="200dp"
          android:layout_height="40dp"
          android:layout_marginTop="24dp"
          android:background="@drawable/gradient_button"
          android:backgroundTintMode="@null"
          android:fontFamily="@font/abril_fatface"
          android:text="Add City"
          android:textColor="@color/black"
          app:backgroundTint="@null"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.924"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/roundtripendcity" />

      <Button
          android:id="@+id/roundtrippriceBTN"
          android:layout_width="320dp"
          android:layout_height="50dp"
          android:layout_marginTop="40dp"
          android:layout_marginBottom="20dp"
          android:background="@drawable/gradient_button"
          android:backgroundTintMode="@null"
          android:fontFamily="@font/abril_fatface"
          android:text="@string/get_prices"
          android:textColor="@color/black"
          app:backgroundTint="@null"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintHorizontal_bias="0.494"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/roundtripaddcityBTN" />

